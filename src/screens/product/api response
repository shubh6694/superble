 RequestObjectLoginOrSignup requestObjectLoginOrSignup = new RequestObjectLoginOrSignup();
        requestObjectLoginOrSignup.first_name = first_name;
        requestObjectLoginOrSignup.last_name = last_name;
        requestObjectLoginOrSignup.user_name = user_name;
        requestObjectLoginOrSignup.email = email;
        requestObjectLoginOrSignup.password = password;
        requestObjectLoginOrSignup.imgFile = img;
        requestObjectLoginOrSignup.public_id = public_id;
        requestObjectLoginOrSignup.phash = phash;
        requestObjectLoginOrSignup.device_id = deviceId;
        requestObjectLoginOrSignup.referral_code = referral_code;

for normal signup use @POST("/sessions")


